set_cc31=106 label_cc31=Џ31:Dive Bomb ₈₃  // ❶
// CC36 to 47 control per-pitch-class tuning. All start centered (50%) and swing both ways.
set_cc36=63 label_cc36=Џ36:Tune C ₅₀      // ░░░░░░ ❶
set_cc37=63 label_cc37=Џ37:Tune C♯/D♭ ₅₀   //   ▓▓▓▓
