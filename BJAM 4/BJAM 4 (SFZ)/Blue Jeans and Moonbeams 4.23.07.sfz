lfo01_fade_oncc4=$SHAKETIME
amplitude_oncc7=100 amplitude_curvecc7=4 // default for CC7, specifying avoids glitchiness.
pan_oncc10=0 // Override the use of CC10 to perform panning, so it can be re-used.
pitch_curvecc10=1 // Pick Noise Tuning starts centered, goes both ways.
ampeg_delaycc14=0.05 // Delay of notes to make room for Autopick.
pitch_curvecc31=7 // Custom Dive Bomb/Neck Dive curve. 83% is neutral.
// CC36 to 47 control per-pitch-class tuning, all are center-focused linear.
  pitch_curvecc36=1 pitch_curvecc37=1 pitch_curvecc38=1 pitch_curvecc39=1
  pitch_curvecc40=1 pitch_curvecc41=1 pitch_curvecc42=1 pitch_curvecc43=1
  pitch_curvecc44=1 pitch_curvecc45=1 pitch_curvecc46=1 pitch_curvecc47=1
// CC48 to 59 control per-pitch-class volume, all are center-focused linear.
  volume_curvecc48=1 volume_curvecc49=1 volume_curvecc50=1 volume_curvecc51=1
  volume_curvecc52=1 volume_curvecc53=1 volume_curvecc54=1 volume_curvecc55=1
  volume_curvecc56=1 volume_curvecc57=1 volume_curvecc58=1 volume_curvecc59=1
// delay curves for pickup phasing: linear is simplest, but x² gives more range
  delay_curvecc60=4 delay_curvecc61=4 delay_curvecc62=4 
ampeg_attackcc73=1 // Max roll-in of notes based on CC73 (time in seconds).
sostenuto_sw=off
// Bend range for primary Pitch Bend, both directions.
