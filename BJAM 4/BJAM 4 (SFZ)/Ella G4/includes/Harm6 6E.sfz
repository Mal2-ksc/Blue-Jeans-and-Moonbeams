// Ella G: Harmonic 3, reserving 6E sample for extended range (5A stands in). Nashville an octave up. ampeg_attack softens attack as Harmonic 2 samples are recycled.
<region> 
delay_cc60=$MAXPUPPHASE
sample=5A-harm2.ogg 
pitch_keycenter=45 
ampeg_attack=0.03 
amplitude_cc16=100 
pitch_cc31=$WHAMMY6E 
xfout_locc72=0 
xfout_hicc72=127 
tune=1901.6

<region> 
delay_cc61=$MAXPUPPHASE
sample=2B-harm2.ogg 
pitch_keycenter=47 
ampeg_attack=0.03 
amplitude_cc17=100 
pitch_cc19=$MAXDETUNE 
pitch_cc31=$WHAMMY2B 
xfout_locc72=0 
xfout_hicc72=127 
tune=1901.6

<region> 
delay_cc61=$MAXPUPPHASE
sample=2B-harm2.ogg 
pitch_keycenter=47 
ampeg_attack=0.03 
amplitude_cc18=100
phase=invert 
pitch_cc19=$MAXDETUNE 
pitch_cc31=$WHAMMY2B 
xfout_locc72=0 
xfout_hicc72=127 
tune=1901.6

